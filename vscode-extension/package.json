{
  "name": "sqlite-database-manager",
  "displayName": "SQLite Database Manager",
  "description": "A lightweight SQLite database manager for VS Code",
  "version": "0.1.0",
  "publisher": "example-publisher",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Database"
  ],
  "keywords": [
    "sqlite",
    "database",
    "manager"
  ],
  "activationEvents": [
    "onCommand:extension.openDatabaseManager"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.openDatabaseManager",
        "title": "SQLite: Open Database Manager"
      },
      {
        "command": "extension.showDatabaseLogs",
        "title": "SQLite: Show Logs"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "extension.openDatabaseManager"
        },
        {
          "command": "extension.showDatabaseLogs"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "./build-binaries.sh && npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "build:binaries": "./build-binaries.sh"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^16.11.8",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.5.2",
    "vsce": "^2.0.0"
  },
  "files": [
    "out/**/*",
    "bin/**/*",
    "media/**/*"
  ]
}
